{
  "extends": "@tsconfig/react-native/tsconfig.json",     /* Recommended React Native TSConfig base */
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "@appTypes/*": ["./appTypes/*"],
      "@constants/*": ["./constants/*"],
      "@components/*": ["./components/*"],
      "@navigators/*": ["./navigators/*"],
      "@screens/*": ["./screens/*"]
    }                               
    /* Skip type checking all .d.ts files. */
  },
}
